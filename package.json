{
  "name": "@contextprotocol/sdk",
  "version": "1.0.5",
  "description": "Context Protocol SDK",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "dist/**/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/contextprotocol/sdk.git"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=18.x",
    "npm": ">=9.x"
  },
  "scripts": {
    "release": "npm run format && npm run build && npm run generate:types",
    "build": "rm -rf dist && npx tsc",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write '*/**/*.{js,json,md,ts}'",
    "format:check": "prettier --check '*/**/*.{js,json,md,ts}'",
    "generate:types": "tsc --emitDeclarationOnly --declaration --outDir dist"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.8",
    "formdata-node": "^6.0.3",
    "mime-types": "^2.1.35",
    "typescript-json-schema": "^0.63.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.4"
  }
}
